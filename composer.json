{
    "name": "iexbase/web3.php",
    "description": "Ethereum web3 interface.",
    "type": "library",
    "license": "MIT",
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "require": {
        "php": "^8.1",
        "ext-bcmath": "*",
        "ext-gmp": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.0",
        "kornrunner/keccak": "~1.0",
        "phpseclib/phpseclib": "^3.0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Web3\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "test/"
        }
    }
}
